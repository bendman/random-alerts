{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "es6": true,
    "node": true,
    "amd": true,
    "browser": true
  },
  "rules": {
    "quotes": [1, "single"],
    "camelcase": 0,
    "react/jsx-uses-react": 1
  },
  "globals": {
    "cordova": true
  }
}
